# E2E Test Environment Variables
# Copy this file to .env.test and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SECRET_KEY=your_supabase_secret_key

# E2E Test User Credentials
# Create a test user in your Supabase project and add credentials here
E2E_USERNAME_ID=test-user-uuid-from-supabase-auth-users-table
E2E_USERNAME=test@example.com
E2E_PASSWORD=YourTestPassword123!

# OpenRouter.ai Configuration (optional for E2E tests)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Development Mode
USE_MOCK_AI=true
